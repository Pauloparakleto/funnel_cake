<h1>Editing funnel_ignore</h1>

<% form_for(@funnel_ignore) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :ip %><br />
    <%= f.text_field :ip %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @funnel_ignore %> |
<%= link_to 'Back', funnel_ignores_path %>
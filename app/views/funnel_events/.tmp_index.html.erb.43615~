<%
cmd = "dot -Txdot "
dotcode = render :partial=>'graph_dot'
logger.debug(dotcode)
f = IO.popen(cmd, "w+")
f.write(dotcode + "\n")
f.close_write
jstext = escape_javascript(f.read)
%>

<script type="text/javascript">
var canviz;
document.observe('dom:loaded', function() {
	canviz = new Canviz('graph_container');
	canviz.setImagePath('/images/');
	canviz.setScale(0.5);
	var txt = "<%=jstext%>";
	//canviz.load('/test.xdot');
	canviz.parse(txt);
});
</script>
<h3>Funnel Events</h3>
<div id="graph_container" style="border: 1px solid;"></div>
<div id="debug_output"></div>

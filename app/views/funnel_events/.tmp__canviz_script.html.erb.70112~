<% graph_container = 'graph_container' unless local_assigns[:graph_container].nil? %>
<script type="text/javascript">
var canviz;
document.observe('dom:loaded', function() {
	canviz = new Canviz('<%=graph_container%>');
	canviz.setImagePath('/images/');
	canviz.setScale(0.5);
	var txt = "<%=xdot_javascript%>";
	canviz.parse(txt);
});
</script>
